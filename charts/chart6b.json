{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
      "text": "Covid-19 new deaths",
      "subtitle": "Sub-title here",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "anchor": "start",
      "color": "black"
    },
    "data": {
      "url": "Hospital_admissions_covid19.csv"},
    "height": 300,
    "width": 310,
    "mark": {"type": "point", "point": false},
    "encoding": {
      "x": {
        "field": "date",
        "type": "temporal",
        "title": null,
        "axis": {"grid": false}
      },
      "y": {
        "field": "newAdmissions",
        "type": "quantitative",
        "title": null,
        "axis": {"grid": false}
      },
      "color": {
        "field": "areaName",
        "type": "nominal",
        "scale": {"scheme": "set1"},
        "title": "LEGEND TITLE",
        "legend": {"orient": "top-left", "fillColor": "#FcFdFd"}
      }
    }
  }