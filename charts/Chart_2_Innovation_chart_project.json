{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Patents per capita vs population density",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/107SBakst/107SBakst.github.io/main/data/Chart2_complete_data.csv"
  },
  "transform": [
      {"filter": "datum.Patents_per_capita > 0"}
  ],
  "mark": "point",
  "height": 400,
  "width": 600,
  "encoding": {
    "x": {
      "field": "Population_density_by_city",
      "type": "quantitative",
      "scale": {"domain": [0, 28000]},
      "axis": {"grid": false}
    },
    "y": {
      "field": "Patents_per_capita",
      "type": "quantitative",
      "scale": {"domain": [0,1]},
      "axis": {"grid": false}
    }
  }
}